{
  "name": "@studio-freight/lenis",
  "version": "0.0.12",
  "description": "Lenis is a smooth scroll library to normalize the scrolling experience across devices",
  "author": "studio-freight",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studio-freight/lenis.git"
  },
  "main": "src/index.mjs",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite docs",
    "build": "vite build --config vite.config.cjs",
    "preview": "vite preview --config vite.config.cjs"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "vite": "^2.8.0"
  },
  "dependencies": {
    "path": "^0.12.7",
    "sass": "^1.49.8",
    "tiny-emitter": "^2.1.0",
    "virtual-scroll": "^2.2.1"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "bugs": {
    "url": "https://github.com/studio-freight/lenis/issues"
  },
  "homepage": "https://github.com/studio-freight/lenis#readme",
  "directories": {
    "doc": "docs"
  },
  "keywords": [
    "smooth",
    "scroll"
  ]
}
